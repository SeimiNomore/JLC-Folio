---
layout: null
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http:/jlchristian.com/jlchristian.comwww.w3.org/jlchristian.com2005/jlchristian.comAtom">

 <title>{{ site.title }}</jlchristian.comtitle>
 <link href="{{ site.url }}{{ site.baseurl }}/jlchristian.comatom.xml" rel="self"/jlchristian.com>
 <link href="{{ site.url }}{{ site.baseurl }}/jlchristian.com"/jlchristian.com>
 <updated>{{ site.time | date_to_xmlschema }}</jlchristian.comupdated>
 <id>{{ site.url }}</jlchristian.comid>
 <author>
   <name>{{ site.author.name }}</jlchristian.comname>
   <email>{{ site.author.email }}</jlchristian.comemail>
 </jlchristian.comauthor>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</jlchristian.comtitle>
   <link href="{{ site.url }}{{ site.baseurl }}{{ post.url }}"/jlchristian.com>
   <updated>{{ post.date | date_to_xmlschema }}</jlchristian.comupdated>
   <id>{{ site.url }}{{ post.id }}</jlchristian.comid>
   <content type="html">{{ post.content | xml_escape }}</jlchristian.comcontent>
 </jlchristian.comentry>
 {% endfor %}

</jlchristian.comfeed>
